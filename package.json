{
  "name": "asset-management-system",
  "version": "1.0.0",
  "description": "Comprehensive Asset Management System for central administrators",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "aws-sdk": "^2.1692.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.2.0",
    "joi": "^17.13.3",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "asset-management",
    "maintenance",
    "dashboard",
    "admin"
  ],
  "author": "Asset Management Team",
  "license": "MIT"
}
